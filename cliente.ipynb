   "source": [
    "import socket\n",
    "#CLIENTE\n",
    "host = '127.0.0.1'\n",
    "porta = 50000\n",
    "caminho = (host, porta)\n",
    "\n",
    "s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)\n",
    "s.connect(caminho)\n",
    "s.sendall(str.encode(\"DALHE DOG MAU\"))\n",
    "\n",
    "dados = s.recv(1024)\n",
    "print(dados.decode())\n",
    "\n",
    "s.close()"
   ]
  }
